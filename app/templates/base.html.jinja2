<!doctype html>
<html lang="en">

  <head>
    <link rel="stylesheet" href="{{ url_for('static', filename='style.css') }}">
    <link rel="stylesheet" href="https://unpkg.com/bootstrap@4.1.3/dist/css/bootstrap.css">
    <title>{% block title %}{% endblock %}</title>
  </head>

  <body>

    {% block content %}
    {% endblock %}

    <script src="https://unpkg.com/vue@2.5.21/dist/vue.js"></script>
    <script src="https://unpkg.com/bootstrap-vue@2.0.0-rc.11/dist/bootstrap-vue.common.js"></script>
    <script src="https://unpkg.com/axios@0.2.1/dist/axios.js"></script>
    {% block javascript %}
    {% endblock %}

  </body>
</html>
